    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "lib": ["ESNext"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["*"],
    "exclude": ["node_modules"]
}